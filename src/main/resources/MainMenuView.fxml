<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="MainMenu" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="400.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="oopdevelopgradle.controller.MainMenuController">
	<children>
		<Label layoutX="156.0" layoutY="8.0" prefHeight="140.0"
			prefWidth="289.0" text="Prof vs Students: Main Menu"
			textAlignment="CENTER" textFill="#15e144" wrapText="true">
			<font>
				<Font size="36.0" />
			</font>
		</Label>
		<Button layoutX="222.0" layoutY="169.0" mnemonicParsing="false"
			onAction="#newGame" prefHeight="31.0" prefWidth="157.0"
			text="New Game" />
		<Button layoutX="222.0" layoutY="210.0" mnemonicParsing="false"
			onAction="#helpGame" prefHeight="31.0" prefWidth="157.0"
			text="How To Play?" />
		<Button layoutX="222.0" layoutY="253.0" mnemonicParsing="false"
			onAction="#exitGame" prefHeight="31.0" prefWidth="157.0"
			text="Exit Game" />
	</children>
</AnchorPane>
